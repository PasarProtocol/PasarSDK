{
  "name": "@PasarProtocol/PasarSDK",
  "version": "1.0.0",
  "description": "PasarProtocol NFT Marketplace SDK",
  "exports": {
    "node": "./dist/pasarSDK.js",
    "browser": {
      "import": "./dist/es/pasarSDK.browser.js"
    }
  },
  "main": "./dist/pasarSDK.js",
  "module": "./dist/es/pasarSDK.js",
  "typings": "typings/",
  "browser": {
    "./dist/pasarSDK.js": "./dist/pasarSDK.browser.js",
    "./dist/es/pasarSDK.js": "./dist/es/pasarSDK.browser.js"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:release",
    "build:debug": "npm run clean && npm run types && rollup -c",
    "build:release": "npm run lint && npm run clean && npm run types && rollup -c --environment prodbuild",
    "dev": "rollup -c -w",
    "clean": "rm -rf ./dist ./typings ./generated",
    "types": "npm run clean && tsc --emitDeclarationOnly --outDir typings/ --project tsconfig.types.json",
    "tsc:build": "rollup -c",
    "tsc:w": "tsc -w",
    "docs": "npx typedoc src/index.ts --excludePrivate --excludeProtected --excludeInternal",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:node": "JEST_CIRCUS=1 jest --silent=false --verbose --runInBand --config jest.node.config.ts",
    "test:node:debug": "JEST_CIRCUS=1 node --inspect-brk node_modules/.bin/jest --runInBand --config jest.node.config.ts",
    "test:node-no-jest": "rollup -c rollup.no-jest.config.js && node generated/tests-no-jest.js",
    "test:browser": "npm run clean && rollup -c rollup.browsertests.config.js",
    "test:node:profile": "JEST_CIRCUS=1 node --prof --no-logfile-per-isolate ./node_modules/.bin/jest --silent=false --verbose --runInBand --config jest.node.config.ts -- crypto/hdkey.test && node --prof-process v8.log > processed.log && echo '' && echo 'Please open processed.log'. && echo ''"
  },
  "dependencies": {
    "@types/fs-extra": "^9.0.13",
    "axios": "^0.24.0",
    "browserfs": "^1.4.3",
    "buffer": "^5.6.0",
    "cd": "^0.3.3",
    "dayjs": "^1.10.4",
    "postcss": "^8.4.4",
    "pretend": "^3.1.0",
    "promise-queue": "^2.2.5",
    "tslib": "^2.3.1",
    "url": "^0.11.0",
    "web3": "^1.6.0"
  },
  "devDependencies": {
    "typescript": "4.5.5",
    "tslib": "2.3.1",
    "@types/bn.js": "5.1.0",
    "@types/node": "14.17.34",
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-class-properties": "7.16.0",
    "@babel/plugin-proposal-decorators": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-typescript": "7.16.0",
    "rollup": "2.67.2",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-commonjs": "19.0.2",
    "@rollup/plugin-eslint": "8.0.1",
    "@rollup/plugin-inject": "4.0.3",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "2.4.2",
    "@rollup/plugin-typescript": "8.3.0",
    "rollup-plugin-analyzer": "4.0.0",
    "rollup-plugin-copy-assets": "2.0.3",
    "rollup-plugin-multi-input": "1.3.1",
    "rollup-plugin-natives": "0.7.5",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-polyfill-node": "0.6.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.5.2",
    "eslint": "8.9.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-es": "4.1.0",
    "eslint-plugin-promise": "6.0.0",
    "rollup-plugin-size": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "5.11.0",
    "@typescript-eslint/parser": "5.11.0",
    "path-browserify": "1.0.1"
  },
  "files": [
    "dist",
    "typings"
  ],
  "include": [
    "typings",
    "dist"
  ],
  "keywords": [
    "pasar",
    "sdk",
    "nft",
    "marketplace"
  ],
  "author": "",
  "license": "MIT",
  "repository": "git@github.com:PasarProtocol/PasarSDK.git",
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint"
    ],
    "tests/src/**/*.{js,ts,tsx}": [
      "eslint"
    ]
  }
}
